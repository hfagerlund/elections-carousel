{
  "name": "elections-carousel",
  "version": "2.0.0",
  "description": "Carousel presentation of data from a JSON feed of election results",
  "main": "./src/js/elections/main.js",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "eslint": "^3.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-prettier": "^2.4.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "node-sass": "^4.5.1",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "polyjuice": "^2.2.0",
    "prettier": "1.9.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "author": "Heini Fagerlund",
  "license": "MIT",
  "homepage": "https://hfagerlund.github.io/elections-carousel/",
  "repository": {
    "type": "git",
    "url": "https://github.com/hfagerlund/elections-carousel.git"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "mustache": "^2.3.0"
  },
  "scripts": {
    "build:production": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --display-error-details --display-modules --display-chunks",
    "coverage": "istanbul cover tape -- test/**/*.test.js",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "eslint-check": "./node_modules/eslint/bin/eslint.js --print-config .eslintrc | eslint-config-prettier-check",
    "format": "./node_modules/prettier/bin/prettier.js ignore-path ./.prettierignore --write '**/*.{js,jsx}'",
    "lint:js": "./node_modules/eslint/bin/eslint.js -c .eslintrc --ext .js src/ webpack.*.js --fix",
    "test": "./uncomment.sh && node_modules/.bin/tape --require ignore-styles test/**/*.test.js",
    "travis": "npm run lint:js && npm run test",
    "prebuild:production": "./comment-out.sh && npm run format && npm run lint:js"
  }
}
